    {
      "document_id": "chip_info_001",
      "document_type": "chip_benefits_summary",
      "title": "Children's Health Insurance Program (CHIP) Benefits Overview",
      "content_chunks": [
        {
          "chunk_id": "chunk_004_001",
          "text": "CHIP provides health coverage for children in families with incomes too high for Medicaid but cannot afford private insurance. Income limits vary by state, typically 200-400% of Federal Poverty Level.",
          "embedding": [0.7, 0.8, 0.9, 1.0, 0.1],
          "metadata": {
            "section": "program_overview",
            "page": 1,
            "relevance_keywords": ["children", "income limits", "Medicaid", "private insurance", "poverty level"]
          }
        },
        {
          "chunk_id": "chunk_004_002",
          "text": "CHIP covers doctor visits, immunizations, hospitalizations, emergency services, prescription medications, dental care, and vision care. Some states may require small copayments for certain services.",
          "embedding": [0.8, 0.9, 1.0, 0.1, 0.2],
          "metadata": {
            "section": "covered_benefits",
            "page": 2,
            "relevance_keywords": ["doctor visits", "immunizations", "hospitalizations", "emergency", "prescription", "dental", "vision", "copayments"]
          }
        }
      ],
