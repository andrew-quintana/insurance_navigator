{
  "design_type": "agent_io",
  "version": "1.0",
  "last_updated": "2024-03-19",
  "agents": [
    {
      "name": "Prompt Security Agent",
      "type": "security",
      "function": "Screens raw user input for injection risks or unsafe content",
      "inputs": {
        "type": "user_submitted_text",
        "description": "Raw text input from users"
      },
      "outputs": {
        "primary": "Sanitized prompt",
        "secondary": "threat_detection_logs",
        "format": "text"
      },
      "dependencies": [],
      "categories": ["security", "input_validation"]
    },
    {
      "name": "Patient Navigator Agent",
      "type": "conversation",
      "function": "Front-facing conversational agent; reformulates and routes user requests",
      "inputs": {
        "type": "sanitized_prompt",
        "description": "Cleaned user input from Prompt Security Agent"
      },
      "outputs": {
        "primary": "Reformulated intent",
        "next_agent": "Intent Structuring Agent",
        "format": "structured_data"
      },
      "dependencies": ["Prompt Security Agent"],
      "categories": ["conversation", "routing"]
    },
    {
      "name": "Regulatory Agent",
      "type": "compliance",
      "function": "Monitors all outgoing data for privacy and regulatory compliance",
      "inputs": {
        "type": ["agent_messages", "user_data"],
        "description": "All outgoing content and user information"
      },
      "outputs": {
        "primary": "Redacted or validated content",
        "secondary": "compliance_pass_fail_log",
        "format": "structured_data"
      },
      "dependencies": [],
      "categories": ["compliance", "privacy"]
    },
    {
      "name": "Quality Assurance Agent",
      "type": "validation",
      "function": "Validates system outputs for accuracy, completeness, and usefulness",
      "inputs": {
        "type": ["final_guide", "strategy_steps", "provider_list"],
        "description": "System outputs to be validated"
      },
      "outputs": {
        "primary": "Pass/fail tags",
        "secondary": "retry_escalation_triggers",
        "format": "structured_data"
      },
      "dependencies": [],
      "categories": ["validation", "quality"]
    },
    {
      "name": "Intent Structuring Agent",
      "type": "processing",
      "function": "Verifies uploaded docs against required checklist; flags missing data",
      "inputs": {
        "type": "structured_query",
        "description": "Processed query from Patient Navigator Agent"
      },
      "outputs": {
        "primary": "Formalized task intent tree",
        "format": "tree_structure"
      },
      "dependencies": ["Patient Navigator Agent"],
      "categories": ["processing", "documentation"]
    },
    {
      "name": "Task Requirements Agent",
      "type": "analysis",
      "function": "Determines required documents, data, or rules for each task",
      "inputs": {
        "type": ["intent_tree", "policy_info"],
        "description": "Task intent and policy information"
      },
      "outputs": {
        "primary": "Document checklist",
        "secondary": "structured_requirements",
        "format": "structured_data"
      },
      "dependencies": ["Intent Structuring Agent"],
      "categories": ["analysis", "requirements"]
    },
    {
      "name": "Intake Validation Agent",
      "type": "validation",
      "function": "Verifies uploaded docs against required checklist; flags missing data",
      "inputs": {
        "type": ["user_documents", "requirements"],
        "description": "User documents and requirements from Task Requirements Agent"
      },
      "outputs": {
        "primary": "Pass/fail flag",
        "secondary": "missing_document_list",
        "format": "structured_data"
      },
      "dependencies": ["Task Requirements Agent"],
      "categories": ["validation", "documentation"]
    },
    {
      "name": "Policy Compliance Evaluator Agent",
      "type": "evaluation",
      "function": "Evaluates proposed service access strategies against plan rules and CMS regulations",
      "inputs": {
        "type": ["candidate_strategy", "policy_details", "cms_rules"],
        "description": "Strategy and policy information"
      },
      "outputs": {
        "primary": "Valid/rejected status with justification",
        "secondary": "retry_halt_trigger",
        "format": "structured_data"
      },
      "dependencies": [],
      "categories": ["evaluation", "compliance"]
    },
    {
      "name": "Service Access Strategy Agent",
      "type": "strategy",
      "function": "Iteratively proposes access strategies to fulfill user intent",
      "inputs": {
        "type": ["task_intent", "retry_threshold", "rejection_feedback"],
        "description": "Task information and feedback"
      },
      "outputs": {
        "primary": "Candidate strategy",
        "secondary": ["retry_plan", "fail_escalation"],
        "format": "structured_data"
      },
      "dependencies": [],
      "categories": ["strategy", "planning"]
    },
    {
      "name": "Service Provider Agent",
      "type": "search",
      "function": "Finds local providers that meet strategy criteria and policy constraints",
      "inputs": {
        "type": ["service_need", "user_location", "cms_database_access"],
        "description": "Service requirements and location information"
      },
      "outputs": {
        "primary": "Provider list with metadata",
        "format": "structured_data"
      },
      "dependencies": [],
      "categories": ["search", "provider_matching"]
    },
    {
      "name": "Healthcare Guide Developer Agent",
      "type": "generation",
      "function": "Assembles validated plan and provider info into structured guide logic",
      "inputs": {
        "type": ["strategy_plan", "provider_list"],
        "description": "Validated strategy and provider information"
      },
      "outputs": {
        "primary": "Guide prompt for PDF generator",
        "format": "structured_data"
      },
      "dependencies": ["Service Provider Agent"],
      "categories": ["generation", "documentation"]
    },
    {
      "name": "Guide to PDF Agent",
      "type": "conversion",
      "function": "Converts structured instructions into downloadable and UI-displayed formats",
      "inputs": {
        "type": "guide_content",
        "description": "Structured guide content"
      },
      "outputs": {
        "primary": "PDF file",
        "secondary": "UI-renderable version",
        "format": ["pdf", "ui_components"]
      },
      "dependencies": ["Healthcare Guide Developer Agent"],
      "categories": ["conversion", "documentation"]
    },
    {
      "name": "Database Agent",
      "type": "storage",
      "function": "Stores and retrieves structured user data and logs",
      "inputs": {
        "type": ["validated_inputs", "outputs", "metadata"],
        "description": "All system data to be stored"
      },
      "outputs": {
        "primary": "Storage confirmation",
        "secondary": "retrievable API object",
        "format": "api_response"
      },
      "dependencies": [],
      "categories": ["storage", "data_management"]
    }
  ],
  "metadata": {
    "total_agents": 13,
    "agent_types": [
      "security",
      "conversation",
      "compliance",
      "validation",
      "processing",
      "analysis",
      "evaluation",
      "strategy",
      "search",
      "generation",
      "conversion",
      "storage"
    ],
    "categories": [
      "security",
